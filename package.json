{
  "name": "mat-cloud-react-jsx-webpack-template",
  "version": "1.0.0",
  "description": "react learn. react example",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development BASE_ENV=dev webpack-dev-server -c build/webpack.dev.js",
    "start:test": "cross-env NODE_ENV=development BASE_ENV=test webpack-dev-server -c build/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=development BASE_ENV=prod webpack-dev-server -c build/webpack.prod.js",
    "build:dev": "cross-env NODE_ENV=production BASE_ENV=dev webpack -c build/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=production BASE_ENV=test webpack -c build/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production BASE_ENV=prod webpack -c build/webpack.prod.js",
    "build:analy": "cross-env NODE_ENV=production BASE_ENV=prod webpack -c build/webpack.analy.js"
  },
  "author": "liu-yongyuan",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
